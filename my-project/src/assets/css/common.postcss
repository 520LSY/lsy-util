@keyframes moveLine {
    0% {
        background-position: -60px 1px, calc(100% - 1px) calc(100% + 60px),
            100% calc(100% - 1px), 1px calc(100% + 60px);
    }
    0% {
        background-position: -60px 1px, calc(100% - 1px) -60px,
            100% calc(100% - 1px), 1px calc(100% + 60px);
    }

    23% {
        background-position: -60px 1px, calc(100% - 1px) -60px,
            0px calc(100% - 1px), 1px calc(100% + 60px);
    }

    40% {
        background-position: -60px 1px, calc(100% - 1px) -60px,
            -60px calc(100% - 1px), 1px 60px;
    }

    42.5% {
        background-position: -60px 1px, calc(100% - 1px) -60px,
            -60px calc(100% - 1px), 1px calc(100% - 60px);
    }
    60% {
        background-position: 60px 1px, calc(100% - 1px) -60px,
            -60px calc(100% - 1px), 1px -60px;
    }
    82.5% {
        background-position: 100% 1px, calc(100% - 1px) -60px,
            -60px calc(100% - 1px), 1px -60px;
    }
    97.5% {
        background-position: calc(100% + 60px) 1px, calc(100% - 1px) 60px,
            -60px calc(100% - 1px), 1px -60px;
    }
    100% {
        background-position: calc(100% + 60px) 1px, calc(100% - 1px) 100%,
            -60px calc(100% - 1px), 1px -60px;
    }
}


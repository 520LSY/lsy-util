/* selenc */
.n-select {
    .n-base-selection {
        --n-border-hover: 1px solid #21a0ff !important;
        --n-border-focus: 1px solid #21a0ff !important;
        --n-border-active: 1px solid #21a0ff !important;
        .n-base-selection-tags {
            background-color: transparent !important;
            color: #fff;
            .n-base-selection-tag-wrapper {
                .n-tag {
                    background-color: transparent !important;
                    color: #ffffff;
                    border: none;
                }
            }
        }
        .n-base-close {
            color: white;
        }
    }
}
.v-binder-follower-container {
    .v-binder-follower-content {
        .n-base-select-menu {
            background-color: #081b30 !important;
            .n-scrollbar {
                .n-virtual-list {
                    .v-vl-items {
                        .v-vl-visible-items {
                            .n-base-select-option--pending::before {
                                background-color: transparent !important;
                            }
                            .n-base-select-option--selected {
                                color: #ffffff;
                            }
                            .n-base-select-option {
                                color: #ffffff;
                            }
                        }
                    }
                }
            }

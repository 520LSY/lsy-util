/* 渐变色：白->蓝 */
.w2b {
    background: linear-gradient(180deg, #ffffff 0%, #21a0ff 99.70703125%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}
/* 渐变色：白->绿 */
.w3b {
    background: linear-gradient(180deg, #ffffff 0%, #3de6fb 99.70703125%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
}
/* 左右弹窗关闭按钮 */
.closeBtnTop {
    position: absolute;
    top: 24px;
    right: 28px;
    width: 32px;
    height: 32px;
    cursor: pointer;
    /* background-color: skyblue; */
}
/* 浮动 */
.com_float {
    display: flex;
    align-items: center;
}

/* 标题中按钮样式 */
.titleBtnSel {
    background: linear-gradient(
        0deg,
        rgba(30, 137, 254, 0.2),
        rgba(30, 137, 254, 0)
    ) !important;
    border: 1px solid #1e89fe !important;
    border-radius: 4px !important;
    font-weight: 600 !important;
    color: #ffffff !important;
}

/* 标题中更多 */
.moreBtn {
    float: right;
    font-size: 12px;
    font-family: PingFangBold;
    font-weight: 500;
    color: #00fffb;
    cursor: pointer;
}
::-webkit-scrollbar {
    /*滚动条整体样式*/
    width: 4px; /*高宽分别对应横竖滚动条的尺寸*/
    height: 5px;
}

::-webkit-scrollbar-thumb {
    /*滚动条里面小方块*/
    border-radius: 2px;
    -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
    background: #1c5fac;
}

::-webkit-scrollbar-track {
    /*滚动条里面轨道*/
    -webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
    border-radius: 2px;
    background: #07192e;
}
/* 分页组件样式 */
.n-pagination {
    .n-pagination-item {
        font-family: PingFang SC;
        color: #ccc;
        background-color: transparent;

        .n-base-icon {
            color: #ffffff;
        }
    }

    .n-pagination-item,
    .n-pagination-item--disabled,
    .n-pagination-item--button .n-pagination-item--disabled {
        background-color: rgba(0, 0, 0, 0.3) !important;
        border: 1px solid rgba(27, 133, 213, 0.7) !important;
        /* opacity: 0.5; */
        border-radius: 2px;
    }

    .n-pagination-item--active {
        border: 1px solid #1e89fe !important;
        background: #3782d4 !important;
        color: #fff !important;
    }

    .n-pagination-item:not(.n-pagination-item--disabled):hover {
        color: #1e89fe !important;
    }
}
